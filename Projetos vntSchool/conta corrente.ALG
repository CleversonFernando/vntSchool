Algoritmo "Conta corrente"

Var

   nome: caractere
   agencia, conta: real

Inicio

   escreval("Digite Nome: ")
   leia(nome)
   escreval("Digite Numero da Agencia: ")
   leia(agencia)
   escreval("Digite Numero da Conta: ")
   leia(conta)

   escreval("Nome: " ,nome, "   Agencia: " ,agencia, "   Conta: ", conta)

Fimalgoritmo